   "source": [
    "a,b = map(int, input().split())\n",
    "years = 0\n",
    "\n",
    "while a <= b:\n",
    "    a*=3\n",
    "    b*=2\n",
    "    years+=1\n",
    "    \n",
    "print(years)"
