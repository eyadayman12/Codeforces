   ],
   "source": [
    "x = 0\n",
    "n = int(input())\n",
    "\n",
    "for i in range(n):\n",
    "    k = input()\n",
    "    if k[0] == '+' or k[1] == '+':\n",
    "        x+=1\n",
    "    elif k[0] == '-' or k[1] == '-':\n",
    "        x-=1\n",
    "        \n",
    "print(x)"
   ]
